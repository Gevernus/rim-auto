# API Configuration
VITE_API_URL=http://localhost:8000/api

# Telegram Bot Configuration
VITE_TELEGRAM_BOT_USERNAME=rim_auto_bot
VITE_TELEGRAM_BOT_TOKEN=your_bot_token_here

# Telegram WebApp Dev over tunnel
# Укажите публичный домен вашего туннеля (ngrok/cloudflared), например: my-app.ngrok-free.app
VITE_PUBLIC_HOST=
# Включить режим конфигурации дев-сервера для Telegram WebApp
VITE_TWA=0

# Optional local dev overrides
# VITE_DEV_HOST=localhost
# VITE_DEV_PORT=3000

# Environment
NODE_ENV=development 